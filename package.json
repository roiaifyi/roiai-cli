{
  "name": "roiai-cli",
  "version": "1.0.0",
  "description": "CLI tool for managing AI service usage data",
  "main": "dist/index.js",
  "bin": {
    "roiai-cli": "./dist/index.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run prisma:generate",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "cli",
    "claude",
    "ai",
    "usage",
    "tracking"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/roiai-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/roiai-cli/issues"
  },
  "homepage": "https://github.com/yourusername/roiai-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "prisma/**/*",
    "config/default.json",
    "config/local.example.json",
    "pricing-data.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@jest/globals": "^30.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/config": "^3.3.5",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "express": "^4.21.2",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.10.0",
    "@types/prompts": "^2.4.9",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "config": "^4.0.0",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "uuid": "^11.1.0"
  }
}
